                  Margin="2"
                  ToolTip="Implante" 
                  MinHeight="16" 
                  MinWidth="34"                  
                  SnapsToDevicePixels="True"
                  Tag="1601">
                
                <Viewbox Stretch="Fill">
                    <Canvas Width="22.4689" Height="11.6667" >
                        <Ellipse Width="13.2537" Height="11.5" Canvas.Left="-1.19209e-007" Canvas.Top="0.166664" Stretch="Fill" StrokeLineJoin="Round" Stroke="SteelBlue"/>
                        <Ellipse Width="13.2537" Height="11.5" Canvas.Left="9.21521" Canvas.Top="0" Stretch="Fill" StrokeLineJoin="Round" Stroke="SteelBlue"/>
                    </Canvas>
                </Viewbox>
            </Grid>

        </ItemsControl.Items>        
    </local:ToolBox>

</ResourceDictionary>